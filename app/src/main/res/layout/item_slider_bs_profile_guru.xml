<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/container_bs_profile_guru"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingStart="@dimen/padding_md"
    android:paddingEnd="@dimen/padding_md"
    android:paddingTop="9dp"
    android:paddingBottom="9dp"
    android:layout_margin="@dimen/margin_xsm"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/bg_card_border_red">

    <TextView
        android:id="@+id/tv_bs_profile_guru"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Matematika"
        android:textSize="@dimen/font_sm"
        android:textColor="@color/merah_primary"
        android:fontFamily="@font/work_sans_medium"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>